/* pages/mv/mv.wxss */
.container{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx;
}
.item{
  width: 220rpx;
  margin-top: 20rpx;
}
.item view{
  width: 220rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.name{
  font-size: 14px;
}
.author{
  font-size: 12px;
  color: #979696;
}
image{
  width: 220rpx;
  height: 220rpx;
  border-radius: 20rpx;
}